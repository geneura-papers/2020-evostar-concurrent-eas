library(ggplot2)
library(ggthemes)
data <- read.csv("code/lo-evosoft-results.csv")
ggplot(data,aes(x=Threads,y=Evaluations,group=Population))+GeomTufteboxplot()
ggplot(data,aes(x=Threads,y=Evaluations,group=Population))+geom_tufteboxplot()
View(data)
data$Population <- as.factor(data$Population)
ggplot(data,aes(x=Threads,y=Evaluations,group=Population))+geom_tufteboxplot()
ggplot(data,aes(x=Threads,y=Evaluations,group=Population))+geom_boxplot()
data$Threads <- as.factor(data$Threads)
ggplot(data,aes(x=Threads,y=Evaluations,group=Population))+geom_boxplot()
View(data)
ggplot(data,aes(x=Threads,y=Evaluations,color=Population))+geom_boxplot()
ggplot(data,aes(x=Threads,y=Evaluations,color=Population))+geom_tufteboxplot()
ggplot(data,aes(x=Threads,y=Evaluations,color=Population))+geom_tufteboxplot()+theme_tufte()
data <- read.csv("code/lo-evosoft-results-freija.csv")
data$Population <- as.factor(data$Population)
